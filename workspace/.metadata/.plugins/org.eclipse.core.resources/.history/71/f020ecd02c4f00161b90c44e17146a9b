package com.sapient.day2.inheritance;

public class RegularEmployee extends Employee{
	
	double salary;
	
	
	public void incrementSalary(){
		this.salary = this.salary * 1.1;
	}
	
	public RegularEmployee(int id, String name, double salary) {
		super(id, name);
		this.salary = salary;
	}

	@Override
	public int toString(){
		return 1;
	}



	


	
	

}
